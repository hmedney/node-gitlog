  "license": "BSD",
  "engines": {
    "node": ">=0.11.12"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jshint": "^2.5.6",
    "mocha": "^3.0.2",
    "should": "~11.1.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "lodash.assign": "^4.2.0"
  }
